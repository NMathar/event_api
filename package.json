{
  "name": "event-api",
  "version": "1.0.0",
  "description": "API",
  "repository": "https://github.com/NMonst4/event_api",
  "keywords": [
    "typeorm",
    "webapi",
    "api",
    "web",
    "web api",
    "typescript"
  ],
  "scripts": {
    "test": "PORT=8081 mocha --reporter spec --require ts-node/register 'test/**/*.test.ts' --exit",
    "build": "rimraf build && tsc",
    "debug": "node ./build/index.js",
    "start": "nodemon --exec ts-node src/index.ts",
    "schema:sync" : "ts-node node_modules/typeorm/cli.js schema:sync",
    "schema:migrate" : "ts-node node_modules/typeorm/cli.js migrations:generate -n Migration && ts-node node_modules/typeorm/cli.js migrations:run"
  },
  "author": "NMonst4",
  "license": "ISC",
  "dependencies": {
    "@types/restify": "^5.0.7",
    "mysql": "^2.15.0",
    "nodemon": "^1.17.2",
    "reflect-metadata": "^0.1.12",
    "restify": "^7.0.0",
    "typeorm": "^0.1.16",
    "typescript": "^2.7.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-http": "^3.0.4",
    "@types/mocha": "^2.2.48",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.0.5",
    "ts-node": "^3.3.0"
  }
}
